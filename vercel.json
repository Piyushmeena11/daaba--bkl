{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/proxy",
      "destination": "/api/proxy.js"
    },
    {
      "source": "/api",
      "destination": "/api/index.js"
    }
  ]
}
